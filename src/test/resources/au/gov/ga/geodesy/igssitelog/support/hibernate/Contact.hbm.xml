<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class
    name="au.gov.ga.geodesy.igssitelog.domain.model.Contact"
    table="SITELOG_CONTACT">
    <id
      name="id"
      type="java.lang.Integer">
      <column
        name="CONTACTNO"
        precision="9"
        scale="0" />
	  
	  <generator class="sequence">
		  <param name="sequence">SEQ_CONTACT</param>
	  </generator>
    </id>

    <property name="name" column="NAME"/>
    <property name="telephonePrimary" column="TELEPHONE_PRIMARY"/>
    <property name="telephoneSecondary" column="TELEPHONE_SECONDARY"/>
    <property name="fax" column="FAX"/>
    <property name="email" column="EMAIL"/>

  </class>
</hibernate-mapping>
