<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class
    name="au.gov.ga.geodesy.igssitelog.domain.model.FrequencyStandard"
    table="SITELOG_FREQUENCYSTANDARD">
    <id
      name="id"
      type="java.lang.Integer">
      <column
        name="FREQUENCY_STANDARDNO"
        precision="9"
        scale="0" />
	  
	  <generator class="sequence">
		  <param name="sequence">SEQ_FREQUENCYSTANDARD</param>
	  </generator>
    </id>

    <component name="effectiveDates" class="au.gov.ga.geodesy.igssitelog.domain.model.EffectiveDates">
       <property name="from" column="EFFECTIVE_FROM"/> 
       <property name="to" column="EFFECTIVE_TO"/>
    </component>

    <property name="standardType" column="STANDARD_TYPE"/>
    <property name="inputFrequency" column="INPUT_FREQUENCY"/>
    <property name="notes" column="NOTES" length="1024"/>

  </class>
</hibernate-mapping>
