<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class
    name="au.gov.ga.geodesy.igssitelog.domain.model.OtherInstrumentation"
    table="SITELOG_OTHERINSTRUMENTATION">
    <id
      name="id"
      type="java.lang.Integer">
      <column
        name="OTHER_INSTRUMENTATIONNO"
        precision="9"
        scale="0" />

     <generator class="sequence">
        <param name="sequence">SEQ_OTHERINSTRUMENTATION</param>
     </generator>
    </id>

    <property name="instrumentation" column="INSTRUMENTATION"/>

    <component name="effectiveDates" class="au.gov.ga.geodesy.igssitelog.domain.model.EffectiveDates">
       <property name="from" column="EFFECTIVE_FROM"/>
       <property name="to" column="EFFECTIVE_TO"/>
    </component>
  </class>
</hibernate-mapping>
