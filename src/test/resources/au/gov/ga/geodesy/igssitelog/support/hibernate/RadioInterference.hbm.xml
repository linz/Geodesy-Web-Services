<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class
    name="au.gov.ga.geodesy.igssitelog.domain.model.RadioInterference"
    table="SITELOG_RADIOINTERFERENCE">
    <id
      name="id"
      type="java.lang.Integer">
      <column
        name="RADIO_INTERFERENCENO"
        precision="9"
        scale="0" />

     <generator class="sequence">
        <param name="sequence">SEQ_RADIOINTERFERENCE</param>
     </generator>
    </id>

    <property name="possibleProblemSource" column="POSSIBLE_PROBLEM_SOURCE"/>
    <property name="observedDegradation" column="OBSERVED_DEGRADATION"/>
    <property name="notes" column="NOTES" length="1024"/>

    <component name="effectiveDates" class="au.gov.ga.geodesy.igssitelog.domain.model.EffectiveDates">
       <property name="from" column="EFFECTIVE_FROM"/>
       <property name="to" column="EFFECTIVE_TO"/>
    </component>
  </class>
</hibernate-mapping>
